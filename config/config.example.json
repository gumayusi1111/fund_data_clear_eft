{
    "baidu_cloud": {
        "remote_path": "/ETF",
        "local_temp_path": "./temp_download",
        "auto_cleanup": true
    },
    "data_processing": {
        "supported_types": [
            "0_ETF日K(前复权)",
            "0_ETF日K(后复权)", 
            "0_ETF日K(除权)"
        ],
        "date_format": "%Y%m%d",
        "encoding": "utf-8"
    },
    "logging": {
        "level": "INFO",
        "file_path": "./logs/etf_sync.log",
        "max_size": "10MB",
        "backup_count": 5
    },
    "sync_settings": {
        "auto_detect_current_month": true,
        "verify_data_integrity": true,
        "backup_before_merge": false
    }
} 